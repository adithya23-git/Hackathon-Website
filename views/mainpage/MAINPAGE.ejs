      <!-- Hero Section -->
      <section class="hero">
        <div class="container text-center">
          <img class="hero-logo" src="./Webnox.jpg" alt="Web Nox">
          <h1 class="display-4">Welcome to Web Nox</h1>
          <p class="lead">Join us at the Web Nox Hackathon, where innovation meets creativity! Collaborate with fellow tech
            enthusiasts and create cutting-edge web solutions. Unleash your potential and shape the future of the web.</p>
        </div>
      </section>

      <!-- Organizer Section -->
      <section class="organizer-section container">
        <h2>Organizer Dashboard</h2>
        <p>Welcome, Organizer! Here you can manage your hackathons, review submissions, and engage with participants.</p>

        <!-- Dashboard Buttons -->
        <div class="d-flex flex-column align-items-center">
          <button class="btn btn-primary rounded-pill dashboard-button" type="button" 
            style="width: 340px; height: 50px; font-size: 1.2em;"><a href="organizehackathon" style="text-decoration: none; color: azure;" > Create New Hackathon </a></button>
          <button class="btn btn-primary rounded-pill dashboard-button" type="button"
            style="width: 340px; height: 50px; font-size: 1.2em;"><a href="projectsub" style="text-decoration: none; color: azure;" > Review Submissions </a></button>
